POSTGRES_PORT=5432
POSTGRES_DB=llm_gateway

# Provider API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Rate Limiting
DEFAULT_RATE_LIMIT=100

# Cache
CACHE_TTL=3600
ENABLE_PROMPT_CACHE=true

# Logging
LOG_LEVEL=INFO
