# API Configuration
SECRET_KEY=your-secret-key-here
API_V1_STR=/api/v1

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-db-password
